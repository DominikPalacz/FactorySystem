{
  "name": "@factory/db",
  "version": "0.0.0",
  "private": true,
  "description": "Drizzle schema and migrations for Factory Nervous System",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./schema": "./dist/schema.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "drizzle:generate": "drizzle-kit generate --config=./drizzle.config.ts",
    "drizzle:push": "drizzle-kit push --config=./drizzle.config.ts",
    "seed": "tsx src/seed.ts"
  },
  "devDependencies": {
    "@types/pg": "^8.11.11"
  },
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.1",
    "pg": "^8.17.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
